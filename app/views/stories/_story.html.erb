<div id="<%= dom_id story %>">
  <p>
    <strong>Title:</strong>
    <%= story.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= story.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= story.user_id %>
  </p>

  <p>
    <strong>Is starred:</strong>
    <%= story.is_starred %>
  </p>

</div>
