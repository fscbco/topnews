<div class="container">
  <% @stories.each do |story| %>
    <div class="row justify-content-md-center">
      <div class="col-8 mt-4">
        <div class="card text-center shadow mb-5 bg-white rounded">
          <div class="card-body">
            <h5 class="card-title"><%= link_to story.title, story.url, class: "text-success" %></h5>
            <h5> <%= link_to "Star", create_star_path(story_id: story.id, user: current.user), class: "btn btn-outline-info" %></h5>
             
              <%= %>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>